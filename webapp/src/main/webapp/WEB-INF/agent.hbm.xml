<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dao.Agent" table="Agent"> 
        <id name="agentId" type="int">
            <column name="agentId" />
            <generator class="increment" />
        </id>
        <property name="agentName" type="string">
            <column name="agentName" />
        </property>
		<property name="agentAddress" type="string">
            <column name="agentAddress" />
        </property>
        <property name="agentPhone" type="string">
            <column name="agentPhone" />
        </property>
    </class>
</hibernate-mapping>
